<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    tools:context="com.esh_tech.aviram.barbershop.CustomersListActivity">


    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center"
        android:background="#ffff"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp">

        <ImageButton
            android:id="@+id/imageButton18"
            android:layout_width="50dp"
            android:layout_height="46dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/plus48"
            android:background="@color/background"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="16dp" />

        <ImageButton
            android:id="@+id/imageButton19"
            android:layout_width="55dp"
            android:layout_height="46dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/search48"
            android:background="@color/background"
            tools:layout_editor_absoluteX="83dp"
            tools:layout_editor_absoluteY="16dp" />

        <EditText
            android:id="@+id/editText6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ems="10"
            android:gravity="start"
            android:inputType="textPersonName"
            android:text="@string/search"
            tools:layout_editor_absoluteX="153dp"
            tools:layout_editor_absoluteY="16dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="349dp"
        android:layout_height="386dp"
        android:layout_weight="1"
        android:background="#ffff"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="23dp"
        tools:layout_editor_absoluteY="84dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="8dp">

            <ImageButton
                android:id="@+id/imageButton24"
                android:layout_width="59dp"
                android:layout_height="match_parent"
                android:layout_weight="0.06"
                android:background="#ffff"
                app:srcCompat="@drawable/phone48" />

            <TextView
                android:id="@+id/textView27"
                android:layout_width="wrap_content"
                android:layout_height="71dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="ליטל כהן" />

            <ImageButton
                android:id="@+id/imageButton23"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0.13"
                android:background="#ffff"
                app:srcCompat="@drawable/userfemale48" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:orientation="horizontal"
            android:weightSum="1"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="8dp">

            <ImageButton
                android:id="@+id/imageButton22"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:background="#ffff"
                app:srcCompat="@drawable/phone48"
                android:layout_weight="0.18" />

            <TextView
                android:id="@+id/textView231"
                android:layout_width="132dp"
                android:layout_height="match_parent"
                android:layout_weight="0.64"
                android:gravity="center"
                android:text="רן פלק" />

            <ImageButton
                android:id="@+id/imageButton201"
                android:layout_width="wrap_content"
                android:layout_height="66dp"
                android:layout_weight="0.18"
                android:background="#ffff"
                app:srcCompat="@drawable/usermale48" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="8dp">

            <ImageButton
                android:id="@+id/imageButton124"
                android:layout_width="59dp"
                android:layout_height="match_parent"
                android:background="#ffff"
                app:srcCompat="@drawable/phone48"
                android:layout_weight="0.06" />

            <TextView
                android:id="@+id/textView271"
                android:layout_width="wrap_content"
                android:layout_height="71dp"
                android:layout_weight="1"
                android:text="נטלי לוי"
                android:gravity="center"/>

            <ImageButton
                android:id="@+id/imageButton123"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:background="#ffff"
                app:srcCompat="@drawable/userfemale48"
                android:layout_weight="0.13" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="8dp"
            android:weightSum="1">

            <ImageButton
                android:id="@+id/imageButton212"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:background="#ffff"
                app:srcCompat="@drawable/phone48"
                android:layout_weight="0.22" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="132dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="אלון משה"
                android:layout_weight="0.65" />

            <ImageButton
                android:id="@+id/imageButton20"
                android:layout_width="wrap_content"
                android:layout_height="61dp"
                android:background="#ffff"
                app:srcCompat="@drawable/usermale48"
                android:layout_weight="0.18" />

        </LinearLayout>
    </LinearLayout>

</android.support.constraint.ConstraintLayout>
